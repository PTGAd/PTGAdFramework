# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.0'

target 'PTGSDKExample' do
  pod 'PTGAdFramework', '2.2.0'   # 需要使用Fancy广告能力的添加此项  PTGAdFramework SDK 1.5.1版本支持
  pod 'FancyAd','1.1.80'
  pod 'UBiXMerakSDK','2.5.0.0002' # ubix 消耗方支持，可选
  # 支持topon 需要导入
  #  use_frameworks! :linkage => :static # 单独设置静态库 PTGAdFramework SDK 2.0.7版本支持
  pod 'AnyThinkPTGAdSDKAdapter', '1.0.3'
  
  # 美约SDK依赖的三方库
  pod 'SDWebImage'
  pod 'WechatOpenSDK'
  pod 'CocoaAsyncSocket'
  
##  
##  pod 'Ads-CN-Beta', '4.8.0.3'                  # 需要使用穿山甲广告能力的添加此项
##  pod 'GDTMobSDK', '4.14.10'            # 需要使用广点通广告能力的添加此项
##  pod 'KSAdSDK', '3.3.13'               # 需要使用快手广告能力的添加此项    PTGAdFramework SDK 1.5.3版本支持
##  pod 'PTGJAdSDK','1.2.0'                # 需要使用京东广告能力的添加此项    PTGAdFramework SDK 1.5.4版本支持 #
  pod 'Masonry'
  pod 'CocoaDebug'
	pod 'SDWebImage'
  
end
